{
  "name": "mysaferoute-backend",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bunx prisma generate",
    "typecheck": "tsc --noEmit",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "db:seed": "bun prisma/seed.ts",
    "db:reset": "bunx prisma db push --force-reset && bun prisma/seed.ts"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "pino-pretty": "^13.1.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/websocket": "^11.2.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/pg": "^8.15.6",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "pg": "^8.16.3",
    "prisma": "^7.0.1",
    "zod": "^4.1.13"
  }
}
